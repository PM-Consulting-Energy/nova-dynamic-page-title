{
    "name": "klepak/nova-dynamic-page-title",
    "description": "Dynamically updates Laravel Nova page title based on data from Vue router.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Klepak\\NovaDynamicPageTitle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Klepak\\NovaDynamicPageTitle\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
